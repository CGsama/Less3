{
  "EnableConsole": true,
  "Database": {
    "Filename": "./Less3.db",
    "Type": "Sqlite",
    "Port": 0
  },
  "Server": {
    "DnsHostname": "localhost",
    "ListenerPort": 8000,
    "Ssl": false,
    "HeaderApiKey": "x-api-key",
    "AdminApiKey": "less3admin",
    "RegionString": "us-west-1"
  },
  "Storage": {
    "StorageType": "Disk",
    "DiskDirectory": "./Storage/",
    "TempDirectory": "./Temp/"
  },
  "Logging": {
    "SyslogServerIp": "127.0.0.1",
    "SyslogServerPort": 514,
    "Header": "less3",
    "MinimumLevel": 0,
    "LogHttpRequests": false,
    "ConsoleLogging": true,
    "DiskLogging": false
  },
  "Debug": {
    "DatabaseQueries": false,
    "DatabaseResults": false,
    "Authentication": true,
    "S3Requests": false
  }
}