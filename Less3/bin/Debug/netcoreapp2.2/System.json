{
  "EnableConsole": true,
  "Files": {
    "ConfigDatabase": "./Less3.db"
  },
  "Server": {
    "DnsHostname": "localhost",
    "ListenerPort": 8000,
    "Ssl": false,
    "HeaderApiKey": "x-api-key",
    "AdminApiKey": "less3admin",
    "RegionString": "us-west-1"
  },
  "Storage": {
    "Directory": "./Storage/",
    "TempDirectory": "./Temp/"
  },
  "Syslog": {
    "ServerIp": "127.0.0.1",
    "ServerPort": 514,
    "Header": "less3",
    "MinimumLevel": 1,
    "LogHttpRequests": true, 
    "ConsoleLogging": true
  },
  "Debug": {
    "Database": false,
    "Authentication": false
  }
}
