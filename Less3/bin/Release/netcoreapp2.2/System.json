{
  "EnableConsole": true,
  "Files": {
    "ConfigDatabase": "./Less3.db"
  },
  "Server": {
    "DnsHostname": "localhost",
    "ListenerPort": 8000,
    "BaseDomain": null,
    "Ssl": false,
    "HeaderApiKey": "x-api-key",
    "AdminApiKey": "less3admin",
    "RegionString": "us-west-1"
  },
  "Storage": {
    "Directory": "./Storage/",
    "TempDirectory": "./Temp/"
  },
  "Logging": {
    "SyslogServerIp": "127.0.0.1",
    "SyslogServerPort": 514,
    "Header": "less3",
    "MinimumLevel": 1,
    "LogHttpRequests": false,
    "ConsoleLogging": true,
    "DiskLogging": true,
    "DiskDirectory": "./Logs/"
  },
  "Debug": {
    "DatabaseQueries": false,
    "DatabaseResults": false,
    "Authentication": false,
    "S3Requests": false
  }
}
