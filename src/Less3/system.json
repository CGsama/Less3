{
  "EnableConsole": false,
  "Database": {
    "Type": "sqlite",  
    "Filename": "/data/less3.db",
    "Port": 0,
	  "Debug": {
		"EnableForQueries": false,
		"EnableforResults": false
	  }
  },
  "Server": {
    "DnsHostname": "localhost",
    "ListenerPort": 8000,
    "Ssl": false,
    "HeaderApiKey": "x-api-key",
    "AdminApiKey": "less3admin",
    "RegionString": "us-west-1"
  },
  "Storage": {
    "TempDirectory": "./Temp/",
    "StorageType": "Disk",
    "DiskDirectory": "/data/less3disk/"
  },
  "Logging": {
    "SyslogServerIp": "127.0.0.1",
    "SyslogServerPort": 514,
    "Header": "less3",
    "MinimumLevel": 1,
    "LogHttpRequests": false,
    "ConsoleLogging": false,
    "DiskLogging": false
  },
  "Debug": {
    "DatabaseQueries": false,
    "DatabaseResults": false,
    "Authentication": false,
    "S3Requests": false
  }